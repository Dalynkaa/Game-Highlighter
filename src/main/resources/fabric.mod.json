{
  "schemaVersion": 1,
  "id": "highlighter",
  "version": "${version}",
  "name": "Highlighter",
  "description": "Mod for highlighting and hiding players",
  "authors": [
    "Dalynkaa"
  ],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/highlighter/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.dalynkaa.highlighter.client.HighlighterClient"
    ],
    "main": [
      "me.dalynkaa.highlighter.Highlighter"
    ],
    "modmenu": [
      "me.dalynkaa.highlighter.client.utilities.ModMenuIntegration"
    ]
  },
  "mixins": [
